<script>
YUI({
	combine: true,
	comboBase: 'http://img1.cache.netease.com/service/combo?',
	modules: {
		'ntes-tabs': {
			fullpath: 'http://img1.cache.netease.com/apps/adp11/ntes-tabs/ntes-tabs.js',
			submodules: {
				'ntes-tabs-base': {
					fullpath: 'http://img1.cache.netease.com/apps/adp11/ntes-tabs/ntes-tabs-base.js',
					requires: ['widget', 'arraylist']
				},
				'ntes-tabs-player': {
					fullpath: 'http://img1.cache.netease.com/apps/adp11/ntes-tabs/ntes-tabs-player.js',
					requires: ['plugin', 'ntes-tabs-base']
				},
				'ntes-tabs-transition': {
					fullpath: 'http://img1.cache.netease.com/apps/adp11/ntes-tabs/ntes-tabs-transition.js',
					requires: ['plugin', 'ntes-tabs-base', 'transition']
				}
			}
		},
		'adp-modelsview': {
			fullpath: 'http://img1.cache.netease.com/apps/adp11/adp-modelsview/adp-modelsview.js',
			requires: ['widget', 'widget-parent', 'widget-child', 'substitute', 'io-base', 'json-parse', 'querystring', 'tabview-base', 'cookie', 'overlay']
		},
		'ntes-placeholder': {
			fullpath: 'http://img1.cache.netease.com/apps/adp11/ntes-placeholder/ntes-placeholder.js',
			requires: ['classnamemanager', 'node', 'plugin']
		}
	}
}).use('ntes-tabs', 'adp-modelsview', 'autocomplete', 'cookie', 'overlay', 'ntes-placeholder', function(Y) {
	var SYS_LINK = {
		'iOS': '/iphone/',
		'Android': '/android/',
		'塞班V3': '/s60v3/',
		'塞班V5': '/s60v5/',
		'Java': '/java/'
	};
	
	function isLoginUC () {
		var pInfo = Y.Cookie.get('P_INFO') || null,
			sInfo = Y.Cookie.get('S_INFO') || null,
			logined = false;
			
		if (pInfo && pInfo !== 'null') {
			pInfo = pInfo.split('|');
			logined = '1' === pInfo[2] || '0' === pInfo[2] && !!sInfo; 
		}
		
		return logined;
	}
	
	if ( isLoginUC() ) {
		Y.io('/usercenter/getmodel?_=' + Y.Lang.now(), {
			method: 'GET',
			on: {
				success: function (id, o) {
					data = Y.JSON.parse(o.responseText);
					if (data) {
						Y.Cookie.setSubs('myModel', data, { path: '/', expires: new Date( Y.Lang.now() + 3 * Math.pow(10, 10) ) });
						window.location.href = SYS_LINK[data.sys];
					}
				},
				failure: function () {
					data = Y.Cookie.getSubs('myModel');
					if (data) {
						window.location.href = SYS_LINK[data.sys];
					}
				}
			}
		});
	} else {
		data = Y.Cookie.getSubs('myModel');
		if (data) {
			window.location.href = SYS_LINK[data.sys];
		}
	}
	
	new Y.Tabs({ srcNode: '#index-tabs', triggerEvent: 'mouseover', render: true, plugins: [ Y.Plugin.TabsPlayer, Y.Plugin.TabsTransition ]});
	
	var searchInput = Y.one('#index-search-input');
	
	searchInput.plug(Y.Plugin.Placeholder);
	
	searchInput.plug(Y.Plugin.AutoComplete, {
	    maxResults: 10,
		resultListLocator: 'results',
		resultTextLocator: 'name',
	    source: '/ajax/software/search?query={query}',
	    resultFormatter: function (query, results) {
	    	return Y.Array.map(results, function (result) {
	        	return '<div class="model-search-result">' +
	        		'<img src="' + result.raw.simg + '" class="model-search-img" width="50" height="50" />' +
	               	'<span class="model-search-title">' + result.raw.brand + ' ' + result.raw.model + '</span>' +
	               	'</div>';
	      	});
	    }
	});
	
	searchInput.ac.after('select', function (e) {
	    var result = e.result;
	 	
	 	Y.Cookie.setSubs('myModel', Y.mix({}, result.raw, true, ['id', 'brand', 'model', 'img', 'simg', 'sys']), { path: '/', expires: new Date( Y.Lang.now() + 3 * Math.pow(10, 10) ) });
		if ( isLoginUC() ) {
			Y.io('/usercenter/setmodel?modelid=' + result.raw.id, { method: 'POST' });
		}
		
		window.location.href = SYS_LINK[result.raw.sys];
	});
	
	var brands = 
		<!--#include file="/json/model/phone.json"-->
		modelPanel1 = Y.one('#model-panel-1'),
		modelPanel2 = Y.one('#model-panel-2'),
		modelInput = Y.one('#model-search-input'),
		
		modelImgView = new Y.ImgView({ visible: false, render: '#model-imgview' }),
		
		brandview = new Y.BrandView({
			strings: { step1: '1.选品牌', step2: '2.选型号', step3: '3.确定机型' },
			render: '#model-brandview'
		}),
		
		hotview = new Y.HotView({
			viewer: modelImgView,
			data: hot,
			render: '#model-hotview'
		}),
				    		
		myModel = new Y.MyModel({ imgView: new Y.ImgView({ render: '#model-imgview' }), boundingBox: '#model-submit', disabled: true, render: true }),
		
		modelOvr = Y.one('#model-overlay'),
		
		settingOvr = new Y.Overlay({
			srcNode: modelOvr,
			width: '750px',
			zIndex: 500,
			constrain: 'body',
			visible: false,
			render: true
		}),
		
		mask = Y.Node.create('<div class="yui3-mymodelview-ovrmask"></div>');
		
	mask.hide();
	Y.one('body').insert(mask, 0);
	
	if (Y.UA.ie && Y.UA.ie <= 6) {
		mask.setStyle('position', 'absolute');
		function resizeOverlayMask () {
			mask.setStyles({
				width: Y.DOM.docWidth() + 'px',
				height: Y.DOM.docHeight() + 'px'
			});
		}
		resizeOverlayMask.call(this);
		Y.on('windowresize', function () {
			resizeOverlayMask.call(this);
		}, this);
	};

	modelOvr.one('a.model-overlay-close').on('click', function (e) {
		e.preventDefault();
		
		mask.hide();
		settingOvr.hide();
	}, this);
	
	Y.one('#index-search-trigger').on('click', function (e) {
		e.preventDefault();
		
		mask.show();
		settingOvr.set('centered', true).show();
	});
    
    modelInput.plug(Y.Plugin.AutoComplete, {
	    maxResults: 10,
		resultListLocator: 'results',
		resultTextLocator: 'name',
	    source: '/ajax/software/search?query={query}',
	    resultFormatter: function (query, results) {
	    	return Y.Array.map(results, function (result) {
	        	return '<div class="model-search-result">' +
	        		'<img src="' + result.raw.simg + '" class="model-search-img" width="50" height="50" />' +
	               	'<span class="model-search-title">' + result.raw.brand + ' ' + result.raw.model + '</span>' +
	               	'</div>';
	      	});
	    }
	});
	
	modelInput.ac.after('valueChange', function (e) {
		myModel.set('modelId', null);
	});
		
    modelInput.ac.after('select', function (e) {
	    var result = e.result;
	 	
	 	Y.MyModel.DATA[result.raw.id] = {
			brand: result.raw.brand,
			model: result.raw.model,
			img: result.raw.img,
			simg: result.raw.simg,
			sys: result.raw.sys
		};
	    myModel.set('modelId', result.raw.id);
	});
    
	Y.each(brands, function(brand) {
        brandview.add({ label: brand.label, uri: brand.uri, viewer: modelImgView });
    });
    
    brandview.each(function (brand) {
    	brand.after('modelSelectionChange', function (e) {
    		myModel.set('modelId', e.id || null);
   		});
    });
    
    hotview.after('modelSelectionChange', function (e) {
    	modelInput.ac.set('value', '');
    	myModel.set('modelId', e.id || null);
    });
    
    modelInput.on('focus', function () {
    	hotview.set('modelSelection', null);
    });
    
	Y.on('click', function (e) {
		e.preventDefault();
		
		brandview.deselectAll();
		
		modelPanel1.show();
		modelPanel2.hide();
	}, '#model-trigger-1');
	Y.on('click', function (e) {
		e.preventDefault();
		
		modelInput.ac.set('value', '');
		hotview.set('modelSelection', null);
		
		modelPanel2.show();
		modelPanel1.hide();
	}, '#model-trigger-2');
		
	myModel.on('click', function (e) {
		var modelid = this.get('modelId'),
			data = Y.MyModel.DATA[modelid];
		
		if ( !this.get('disabled') ) {
			Y.Cookie.setSubs('myModel', Y.mix({ id: modelid }, data), { path: '/' });
			if ( isLoginUC() ) {
				Y.io('/usercenter/setmodel?modelid=' + modelid, { method: 'POST' });
			}
			this.get('boundingBox').set( 'href', SYS_LINK[data.sys] );
		}
	});
	
});
